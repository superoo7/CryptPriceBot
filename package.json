{
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env",
    "start": "node dist/index.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "dotenv": "^4.0.0",
    "node-telegram-bot-api": "^0.30.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.14.0",
    "nodemon": "^1.14.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-var": 1
    }
  },
  "engines": {
    "node": "8.9.2"
  },
  "name": "statBot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT"
}
